						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"admin\",\n  \"password\": \"admin123\"\n}"
						}
					}
				}
			]
		},
		{
			"name": "用户管理",
			"item": [
				{
					"name": "创建用户",
					"request": {
						"method": "POST",
						"header": [
							{ "key": "Content-Type", "value": "application/json" },
							{ "key": "Authorization", "value": "Bearer {{token}}" }
						],
						"url": {
							"raw": "{{base_url}}/api/users",
							"host": ["{{base_url}}"],
							"path": ["api", "users"]
						},
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"newuser\",\n  \"password\": \"password123\",\n  \"email\": \"newuser@example.com\"\n}"
						}
					}
				},
				{
					"name": "获取用户列表",
					"request": {
						"method": "GET",
						"header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
						"url": {
							"raw": "{{base_url}}/api/users/list",
							"host": ["{{base_url}}"],
							"path": ["api", "users", "list"]
						}
					}
				},
				{
					"name": "更新用户信息",
					"request": {
						"method": "PUT",
						"header": [
							{ "key": "Content-Type", "value": "application/json" },
							{ "key": "Authorization", "value": "Bearer {{token}}" }
						],
						"url": {
							"raw": "{{base_url}}/api/users/update",
							"host": ["{{base_url}}"],
							"path": ["api", "users", "update"]
						},
						"body": {
							"mode": "raw",
							"raw": "{\n  \"id\": \"{{user_id}}\",\n  \"username\": \"updateduser\",\n  \"email\": \"updated@example.com\"\n}"
						}
					}
				},
				{
					"name": "删除用户",
					"request": {
						"method": "DELETE",
						"header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
						"url": {
							"raw": "{{base_url}}/api/users/delete?id={{user_id}}",
							"host": ["{{base_url}}"],
							"path": ["api", "users", "delete"],
							"query": [{ "key": "id", "value": "{{user_id}}" }]
						}
					}
				}
			]
		},
