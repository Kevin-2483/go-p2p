						"body": {
							"mode": "raw",
							"raw": "{\n  \"url\": \"turn:example.com:3478\",\n  \"username\": \"turnuser\",\n  \"password\": \"turnpass\",\n  \"space_id\": \"{{space_id}}\"\n}"
