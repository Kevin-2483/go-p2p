						},
						"body": {
							"mode": "raw",
							"raw": "{\n  \"account\": \"admin\",\n  \"password\": \"admin123\"\n}"
						}
					}
				},
				{
					"name": "用户登出",
					"request": {
						"method": "POST",
						"url": {
							"raw": "{{base_url}}/api/logout",
							"host": ["{{base_url}}"],
							"path": ["api", "logout"]
						}
					}
				},
				{
					"name": "管理员登出",
					"request": {
						"method": "POST",
						"url": {
							"raw": "{{base_url}}/api/admin/logout",
